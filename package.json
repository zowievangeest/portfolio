{
  "name": "portfolio-zowie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=production && node prod-server/index.js",
    "serve": "vue-cli-service serve",
    "build": "export NODE_ENV=production && babel dev-server --out-dir prod-server && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint '**/*.vue' --syntax scss",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "build-prod-sentry": "export VUE_APP_SENTRY_RELEASE=$(git rev-parse HEAD) && yarn build",
    "dev": "concurrently \"export NODE_ENV=development\" \"babel dev-server --out-dir prod-server --watch\" \"nodemon prod-server/index.js\" \"npm run serve\" "
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "buefy": "^0.7.3",
    "core-js": "^2.6.5",
    "dotenv": "^7.0.0",
    "jsonwebtoken": "^8.5.0",
    "moment": "^2.24.0",
    "mongoose": "^5.4.19",
    "nodemon": "^1.18.10",
    "raven-js": "^3.27.0",
    "register-service-worker": "^1.5.2",
    "url-parse": "^1.4.4",
    "vee-validate": "^2.2.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "@sentry/webpack-plugin": "^1.6.2",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-e2e-cypress": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-pwa": "^3.5.1",
    "@vue/cli-plugin-unit-jest": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-cli": "^6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.4.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "morgan": "^1.9.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recess-order": "^2.0.1",
    "stylelint-config-standard": "^18.2.0",
    "vue-template-compiler": "^2.5.21"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "stylelint '**/*.vue' --syntax scss",
      "git add"
    ]
  }
}
